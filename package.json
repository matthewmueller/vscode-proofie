{
  "name": "writer",
  "displayName": "writer",
  "description": "writer helps you write better",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.68.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "writer.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.4",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint": "^7.19.0",
    "glob": "^7.1.6",
    "mocha": "^8.2.1",
    "typescript": "^4.1.3",
    "vscode-test": "^1.5.0"
  },
  "dependencies": {
    "dictionary-en-gb": "2.4.0",
    "retext-assuming": "1.0.0",
    "retext-english": "4.1.0",
    "retext-equality": "6.6.0",
    "retext-passive": "4.2.0",
    "retext-profanities": "7.2.1",
    "retext-readability": "7.2.0",
    "retext-simplify": "7.2.0",
    "retext-spell": "5.1.0",
    "retext-stringify": "3.1.0",
    "retext-usage": "0.5.0",
    "unified": "10.1.2",
    "vfile-reporter": "7.0.4"
  }
}
